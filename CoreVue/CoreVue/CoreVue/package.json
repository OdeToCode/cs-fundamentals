{
  "name": "core-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "dev": "vue-cli-service build --watch",
    "serve": "vue-cli-service serve",
    "dtos": "cd src && typescript-ref",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "publish": "vue-cli-service build && dotnet publish -c Release",
    "test": "vue-cli-service test:unit",
    "test-watch": "vue-cli-service test:unit --watchAll",
    "e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "@servicestack/client": "^1.0.20",
    "bootstrap": "^4.3.1",
    "es6-shim": "^0.35.5",
    "font-awesome": "^4.7.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-e2e-cypress": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-core": "7.0.0-bridge.0",
    "gulp": "^4.0.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost:5000/"
  }
}
